{
	"publisher": "antfu",
	"name": "iconify",
	"displayName": "Iconify IntelliSense",
	"type": "commonjs",
	"version": "0.7.4",
	"private": true,
	"packageManager": "pnpm@8.13.1",
	"description": "Intelligent Iconify previewing and searching for VS Code",
	"license": "MIT",
	"homepage": "https://github.com/antfu/vscode-iconify",
	"repository": {
		"type": "git",
		"url": "https://github.com/antfu/vscode-iconify"
	},
	"bugs": "https://github.com/antfu/vscode-iconify/issues",
	"keywords": [
		"icons",
		"iconify"
	],
	"sponsor": {
		"url": "https://github.com/sponsors/antfu"
	},
	"preview": true,
	"categories": [
		"Other",
		"Visualization",
		"Snippets"
	],
	"main": "index.js",
	"icon": "res/logo.png",
	"engines": {
		"vscode": "^1.81.0"
	},
	"activationEvents": [
		"workspaceContains:package.json",
		"onLanguage:vue",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:handlebars",
		"onLanguage:svelte",
		"onLanguage:html",
		"onLanguage:css"
	],
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "snippets/html.json"
			},
			{
				"language": "vue",
				"path": "snippets/html.json"
			}
		],
		"commands": [
			{
				"command": "iconify.toggle-annotations",
				"category": "Iconify",
				"title": "Toggle Annotations"
			},
			{
				"command": "iconify.toggle-inplace",
				"category": "Iconify",
				"title": "Toggle In-place Mode"
			},
			{
				"command": "iconify.clear-cache",
				"category": "Iconify",
				"title": "Clear icon cache"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Iconify IntelliSense",
			"properties": {
				"iconify.inplace": {
					"type": "boolean",
					"default": true,
					"description": "Use icon graph to replace the icon name."
				},
				"iconify.annotations": {
					"type": "boolean",
					"default": true,
					"description": "Enabled Iconify inline annotations"
				},
				"iconify.position": {
					"type": "string",
					"enum": [
						"before",
						"after"
					],
					"default": "before",
					"description": "Position the icon before or after the icon name"
				},
				"iconify.color": {
					"type": "string",
					"default": "auto",
					"description": "Icon color hex for inline displaying"
				},
				"iconify.delimiters": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						":",
						"--",
						"-",
						"/"
					],
					"description": "Delimiters for separating between collection id and icon id"
				},
				"iconify.prefixes": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"",
						"i-",
						"~icons/"
					],
					"description": "Prefixes for matching"
				},
				"iconify.suffixes": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"",
						"i-"
					],
					"description": "Suffixes for matching"
				},
				"iconify.languageIds": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"javascript",
						"javascriptreact",
						"typescript",
						"typescriptreact",
						"vue",
						"svelte",
						"html",
						"pug",
						"json",
						"yaml"
					],
					"description": "Array of language IDs to enable annotations"
				},
